{
    "name": "playwright-demo",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
      "test_ui": "playwright test --config=playwright.config.ts",
      "test_ui:cache": "cross-env MIDSCENE_CACHE=true playwright test --config=playwright.config.ts",
      "test_ui:ui": "playwright test --config=playwright.config.ts --ui",
      "test_ui:ui:cache": "cross-env MIDSCENE_CACHE=true playwright test --config=playwright.config.ts --ui",
      "postinstall": "npm exec playwright install"
    },
    "devDependencies": {
      "@midscene/web": "latest",
      "@playwright/test": "1.48.0",
      "@types/jest": "~29.5.14",
      "@types/node": "~22.7.9",
      "cross-env": "7.0.3",
      "dotenv": "16.4.5",
      "eslint-plugin-prettier": "~5.2.1",
      "rimraf": "~6.0.1",
      "typescript": "~5.6.3"
    },
    "publishConfig": {
      "access": "public"
    }
  }